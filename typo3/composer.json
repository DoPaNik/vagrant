{
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.typo3.org/"
    }
  ],
  "require": {
    "typo3/cms": "7.6.6",
    "typo3-ter/extension-builder": "^7.6.0"
  },
  "config": {
    "vendor-dir": "cms/vendor",
    "cache-dir" : "/home/vagrant/.cache"
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": "cms"
    }
  }
}
